<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="create-trainees_trainers" author="IlyaZhizhin">
        <createTable tableName="trainees_trainers">
            <column name="trainer_id" type="int">
                <constraints foreignKeyName="fk_trainer_id" references="trainers(id)" nullable="false" />
            </column>
            <column name="trainee_id" type="int">
                <constraints foreignKeyName="fk_trainee_id" references="trainees(id)" nullable="false" />
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>